block = tag.innerText;\n        return block\n    }\n\n    function convertDate(date) {\n        let dateArray = date.slice(0, 10).split(\"-\")\n        let year = dateArray.shift();\n        dateArray.push(year)\n        console.log(date)\n        return `Published: ${dateArray.join(\"/\")}`;\n    }\n\n    return (\n        <div className=\"PostContainer\">\n            {blogPost()}\n        </div>\n    );\n}\n\nexport default BlogTile;","/home/alina/hello-react/src/components/FooterStyles.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":2,"column":8,"nodeType":"78","messageId":"79","endLine":2,"endColumn":21},{"ruleId":"76","severity":1,"message":"80","line":3,"column":10,"nodeType":"78","messageId":"79","endLine":3,"endColumn":16},{"ruleId":"81","severity":1,"message":"82","line":6,"column":4,"nodeType":"83","endLine":6,"endColumn":68},{"ruleId":"76","severity":1,"message":"84","line":3,"column":10,"nodeType":"78","messageId":"79","endLine":3,"endColumn":15},{"ruleId":"76","severity":1,"message":"85","line":3,"column":34,"nodeType":"78","messageId":"79","endLine":3,"endColumn":40},{"ruleId":"76","severity":1,"message":"86","line":7,"column":5,"nodeType":"78","messageId":"79","endLine":7,"endColumn":12},{"ruleId":"76","severity":1,"message":"87","line":2,"column":9,"nodeType":"78","messageId":"79","endLine":2,"endColumn":13},{"ruleId":"88","severity":1,"message":"89","line":6,"column":5,"nodeType":"90","messageId":"91","endLine":136,"endColumn":6},{"ruleId":"92","severity":1,"message":"93","line":7,"column":16,"nodeType":"83","endLine":7,"endColumn":67},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'HomepageImage' is defined but never used.","Identifier","unusedVar","'Button' is defined but never used.","jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You donâ€™t need to use the words `image`, `photo,`